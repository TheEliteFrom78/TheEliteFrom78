-- sliders n jit lang
Scripts:AddSlider({
	Name = "WalkSpeed",
	Min = 0,
	Max = 500,
	Default = 16,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Value",
	Callback = function(Value)
		_G.sped = Value
        walkspeed()
	end    
})

Scripts:AddSlider({
	Name = "Jump Height",
	Min = 0,
	Max = 500,
	Default = 11,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Value",
	Callback = function(Value)
		_G.jummp = Value
        jummmp()
	end    
})

Scripts:AddSlider({
	Name =  "Gravity",
	Min = 0,
	Max = 196,
	Default = 196,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Value",
	Callback = function(Value)
		_G.grav = Value
